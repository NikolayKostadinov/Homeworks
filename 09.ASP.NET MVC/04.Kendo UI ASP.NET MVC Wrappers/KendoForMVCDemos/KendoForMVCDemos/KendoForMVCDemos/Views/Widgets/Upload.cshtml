
@{
    ViewBag.Title = "Upload";
}

<h2>@ViewBag.Title</h2>

<div class="form-group">
    @(Html.Kendo()
        .Upload()
        .Name("upload")
        .Multiple(true)
        .Async(async =>
        {
            async.AutoUpload(true)
                .Save("UploadedFiles", "Widgets");
        }))
</div>

